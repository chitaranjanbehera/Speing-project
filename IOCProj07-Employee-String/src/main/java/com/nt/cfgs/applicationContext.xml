<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
<bean id="emp" class="com.nt.beans.Employee">
    <constructor-arg name="eno" value="100"/>
    <constructor-arg name="ename" value="Chita"/>
    <constructor-arg name="billAmt" value="4052.54f"/>
</bean>
<bean id="cust" class="com.nt.beans.Customer">
    <constructor-arg name="cno" value="100"/>
    <constructor-arg name="cname" value="Chita"/>
    <constructor-arg name="billAmt" value="4052.54f"/>
    <property name="caddrs" value="HYD"/>
</bean>

</beans>